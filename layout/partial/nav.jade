.flex-left.top-gap
    each value, key in theme.menu
        .unit.site-box
            - var re = /^(http|https):\/\/*/gi;
            - var tar = re.test(value) ? "_blank" : "_self"
            a(class={active: is_home() && value === "/"} href=value target=tar)
                != key.toUpperCase()
hr